{
	"$schema": "vscode://schemas/color-theme",
	"name": "Alabaster",
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#8a290f"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.regexp",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#0031a9"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#4a3a8a"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant.keyword",
				"constant"
			],
			"settings": {
				"foreground": "#721045"
			}
		},
		{
			"name": "Global definitions",
			"scope": "entity.name",
			"settings": {
				"foreground": "#005e8b"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#595959"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"background": "#ff8f88",
				"foreground": "#a60000"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"background": "#DDDDFF",
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"background": "#FFDDDD",
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"background": "#DDFFDD",
				"foreground": "#434343"
			}
		}
	],
	"colors": {
		"editor.background": "#FFFFFF",
		"editor.foreground": "#000",
		"editor.lineHighlightBackground": "#DAE5EC",
		"editor.selectionBackground": "#BDBDBD",
		"editor.selectionHighlightBackground": "#BDBDBD",
		"panel.background": "#F0F0F0",
		"sideBar.background": "#F0F0F0",
		"editorGroupHeader.tabsBackground": "#F0F0F0",
		"activityBar.background": "#F0F0F0",
		"activityBar.foreground": "#007ACC",
		"editorLineNumber.foreground": "#9DA39A",
		"editorCursor.foreground": "#007ACC",
		"editor.findMatchBackground": "#FFBC5D",
		"editor.findMatchHighlightBackground": "#FFD86381",
		"statusBar.background": "#C8C8C8",
		"statusBar.foreground": "#000",
		"statusBar.debuggingBackground": "#f3d000",
		"statusBar.debuggingForeground": "#000",
		"statusBar.noFolderBackground": "#dfa0f0",
		"statusBar.noFolderForeground": "#000",

		"list.activeSelectionBackground": "#DDDDDD",
		"list.activeSelectionForeground": "#000000",
		"list.inactiveSelectionBackground": "#E6E6E6",
		"list.focusHighlightForeground": "#007ACC",
		"focusBorder": "#CCCCCC",

		"quickInputList.focusForeground": "#000000",
		"quickInputList.focusBackground": "#DDDDDD",

		"editorSuggestWidget.selectedBackground": "#DDDDDD",
		"editorSuggestWidget.focusHighlightForeground": "#000000",
		"editorSuggestWidget.highlightForeground": "#000000",
		"editorSuggestWidget.selectedForeground": "#000000",

		"terminal.ansiWhite": "#BBBBBB",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBlue": "#0031a9",
		"terminal.ansiCyan": "#005e8b",
		"terminal.ansiGreen": "#006800",
		"terminal.ansiMagenta": "#721045",
		"terminal.ansiRed": "#a60000",
		"terminal.ansiYellow": "#6f5500",
		"terminal.ansiBrightWhite": "#FFFFFF",
		"terminal.ansiBrightBlack": "#777777",
		"terminal.ansiBrightBlue": "#007ACC",
		"terminal.ansiBrightCyan": "#00AACB",
		"terminal.ansiBrightGreen": "#60CB00",
		"terminal.ansiBrightMagenta": "#E64CE6",
		"terminal.ansiBrightRed": "#F05050",
		"terminal.ansiBrightYellow": "#FFBC5D"
	}
}
